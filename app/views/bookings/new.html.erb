<h1>Nouvelle Réservation pour <%= @van.title %></h1>
<%= form_with model: [@van, @booking], local: true do |form| %>
  <div>
    <%= form.label :start_date %>
    <%= form.date_field :start_date %>
  </div>
  <div>
    <%= form.label :end_date %>
    <%= form.date_field :end_date %>
  </div>
  <div>
    <%= form.submit 'Réserver' %>
  </div>
<% end %>
<%= link_to 'Retour aux Réservations', van_bookings_path(@van) %>
